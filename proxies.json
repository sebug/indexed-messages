{
    "proxies": {
        "staticFront": {
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "/"
            },
            "backendUri": "https://indexedmessages.blob.core.windows.net/indexedmessagesstatic/index.html"
        },
	"serviceworker": {
	    "matchCondition": {
		"methods": [
		    "GET"
		],
		"route": "/build.js"
	    },
	    "backendUri": "https://indexedmessages.blob.core.windows.net/indexedmessagesstatic/build.js"
	}
    }
}
